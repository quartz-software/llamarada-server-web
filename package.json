{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "seed": "cross-env NODE_ENV=dev ts-node ./src/scripts/seed.ts",
    "migrate": "cross-env NODE_ENV=dev ts-node ./src/scripts/migrate.ts",
    "migrate:prod": "cross-env NODE_ENV=production node ./dist/src/scripts/migrate.js",
    "seed:prod": "cross-env NODE_ENV=production node ./dist/src/scripts/seed.js",
    "dev": "cross-env NODE_ENV=dev ts-node-dev ./src/index.ts",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pre:start": "node ./dist/scripts/migrate.js && node ./dist/scripts/seed.js",
    "start": "node ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docs": "jsdoc -c jsdoc.json",
    "docs:swagger": "ts-node src/index.ts",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "uuid": "^11.0.3",
    "zod": "^3.25.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.13",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.6.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.2"
  }
